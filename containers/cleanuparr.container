[Unit]
Description=cleanuparr podman container
Wants=network-online.target
After=network-online.target


[Container]
Image=ghcr.io/cleanuparr/cleanuparr
ContainerName=cleanuparr
Environment=PUID=1001
Environment=PGID=1001
Environment=TZ=Europe/Rome
Volume=/home/shared/.config/cleanuparr:/config:Z
#PublishPort=11011:11011
AutoUpdate=registry
Network=container:protonvpn

[Service]
Restart=always

[Install]
WantedBy=default.target
